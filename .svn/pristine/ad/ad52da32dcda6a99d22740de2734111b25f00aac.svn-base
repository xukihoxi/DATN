        <template id="rate_department_report">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="web.external_layout" t-lang="en_US">
                        <div class="page background_gangnam">
                            <div class="page background_gangnam">
                                <div style="margin:auto;height: auto;width: 700px;font-size: 18px;">
                                    <div style="text-align: center;height: auto;width: 700px;">
                                        <p style="font-size: 25px;">PHIẾU ĐÁNH CÁC BỘ PHẬN</p>
                                    </div>
                                    <div style="height: auto;width: 700px;">
                                        <div>
                                            <div style="float:left;font-size: 25px;">
                                                Thông tin khách hàng
                                            </div>
                                            <div style="float: right">
                                                <div style="width: 100%; height: 30px; padding:0px; margin: 0px">
                                                    <div style="height: 30px; float: right; font-size:18px; font-style:italic;  padding:0px; margin: 0px">
                                                        Ngày
                                                        <span t-esc="context_timestamp(datetime.datetime.now()).strftime('%d')"/>
                                                        tháng
                                                        <span t-esc="context_timestamp(datetime.datetime.now()).strftime('%m')"/>
                                                        năm
                                                        <span t-esc="context_timestamp(datetime.datetime.now()).strftime('%Y')"/>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <br/>
                                        <div style="clear: both;margin: 20px 0px 0px 12px;">Họ và tên:
                                            <span t-field="o.customer_id.name" style="margin-left:5px;"/>
                                        </div>
                                        <div style="clear: both;margin: 7px 0px 0px 12px;">Mã khách hàng:
                                            <span t-field="o.customer_id.x_code" style="margin-left:5px;"/>
                                        </div>
                                        <div style="margin: 7px 0px 0px 12px;">Địa chỉ:
                                            <span t-field="o.customer_id.street" style="margin-left:5px;"/>
                                        </div>
                                        <div style="margin: 7px 0px 0px 12px;">Số điện thoại:
                                            <span t-field="o.customer_id.phone" style="margin-left:5px;"/>
                                        </div>
                                        <!--<div style="margin-top: 7px;">Email:-->
                                        <!--    <span t-field="o.customer_id.email" style="margin-left:5px;"/>-->
                                        <!--</div>-->
                                    </div>
                                    <div style="height: auto;width: 700px;">
                                        <div style="margin-top:10px;">
                                            <div style="font-size: 25px;">
                                                Đánh giá sự hài lòng của khách hàng
                                            </div>
                                            <!--<div style="font-size: 18px;">-->
                                            <!--    <i>(Quý khách hàng tích vào những ô cho là đúng)</i>-->
                                            <!--</div>-->
                                        </div>
                                    </div>


                                    <div t-foreach="o.department_rate_line_ids" t-as="l" style="margin-top:20px;">
                                        <span style="padding:1px; margin-left: 10px;" t-field="l.department_id.name"/>
                                        <div style="width: 100%;">

                                            <div style="width: 700px;margin-left : 51px;float: left;">
                                                <div style="height: 15px;width: 15px; float: left;border: 1px solid #000;margin-top:3px;">
                                                    <t t-if="l.customer_rate == 0">
                                                        <div style="font-size: 25px;margin-top:-13px;">x</div>
                                                    </t>
                                                </div>
                                                <div style="float: left;margin-left: 7px;">
                                                    Chưa hài lòng
                                                </div>
                                                <div style="height: 15px;width: 15px; border: 1px solid #000;margin-left: 50px;float: left;;margin-top:3px;">
                                                    <t t-if="l.customer_rate == 1">
                                                        <div style="font-size: 25px;margin-top:-13px;">x</div>
                                                    </t>
                                                </div>
                                                <div style="float: left;margin-left: 7px;">
                                                    Tạm hài lòng
                                                </div>
                                                <div style="height: 15px;width: 15px; border: 1px solid #000;float: left;margin-left: 50px;;margin-top:3px;">
                                                    <t t-if="l.customer_rate == 2">
                                                        <div style="font-size: 25px;margin-top:-13px;">x</div>
                                                    </t>
                                                </div>
                                                <div style="float: left;margin-left: 7px;">
                                                    Rất hài lòng
                                                </div>
                                                <div style="height: 15px;width: 15px; border: 1px solid #000;float: left;margin-left: 50px;;margin-top:3px;">
                                                    <t t-if="l.customer_rate == 3">
                                                        <div style="font-size: 25px;margin-top:-13px;">x</div>
                                                    </t>
                                                </div>
                                                <div style="float: left;margin-left: 7px;">
                                                    Không đánh giá
                                                </div>
                                                <br/>
                                                <br/>
                                            </div>

                                        </div>
                                    </div>


                                </div>
                            </div>
                        </div>
                    </t>
                </t>
